{
  "name": "quick-music-notation",
  "version": "1.0.0",
  "description": "Browser-based JavaScript library for real-time music notation input via keyboard shortcuts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "serve": "npx http-server . -p 8080 -c-1"
  },
  "keywords": [
    "music",
    "notation",
    "real-time",
    "keyboard",
    "conductor",
    "sheet-music"
  ],
  "author": "Chad Fowler",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "rollup": "^4.9.1",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "jest-environment-jsdom": "^30.0.0-beta.3"
  },
  "peerDependencies": {
    "abcjs": "^6.0.0",
    "opensheetmusicdisplay": "^1.8.0",
    "vexflow": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "vexflow": {
      "optional": true
    },
    "abcjs": {
      "optional": true
    },
    "opensheetmusicdisplay": {
      "optional": true
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": ""
}
